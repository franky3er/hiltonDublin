<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>hiltonDublin</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>home</servlet-name>
  	<jsp-file>/index.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
  	<servlet-name>home</servlet-name>
  	<url-pattern>/Home</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>guest</servlet-name>
  	<jsp-file>/guest.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
  	<servlet-name>guest</servlet-name>
  	<url-pattern>/Guest</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>onlineReservation</servlet-name>
  	<jsp-file>/onlinereservation.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
  	<servlet-name>onlineReservation</servlet-name>
  	<url-pattern>/Guest/Online-Reservation</url-pattern>
  </servlet-mapping>
</web-app>